{
  "name": "name-of-project",
  "description": "Discord bot base made with typescript on discord.js lib version 14.x",
  "version": "0.0.1",
  "main": "dist/index.js",
  "scripts": {
    "start": "node .",
    "dev": "ts-node-dev -r tsconfig-paths/register --transpiler sucrase/ts-node-plugin src/index.ts",
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist"
  },
  "keywords": [
    "bot",
    "discord"
  ],
  "license": "MIT",
  "devDependencies": {
    "@rocketseat/eslint-config": "latest",
    "eslint": "latest",
    "sucrase": "latest",
    "tsconfig-paths": "latest",
    "tscpaths": "latest",
    "typescript": "latest"
  },
  "dependencies": {
    "discord.js": "latest",
    "dotenv": "latest",
    "unix-functions": "latest"
  }
}
